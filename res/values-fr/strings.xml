<string name="log_name">Journal d\'accès</string>
<string name="log_name_format">Journal d\'accès pour {0}</string>

<!-- the following string are used to format a string that indicate when a
     given secret was created or last viewed, modified, or exported.  The
     {0} will be replaced with a "verb" which describes the action, and {1}
     will be replaced with a time value.  Note that for log_min, the time
     value is simply an integer, whereas for the rest its a Date object. -->
<string name="log_sec">{0} il y a quelques instants</string>
<string name="log_min">{0} il y a {1} minutes</string>
<string name="log_today">{0} aujourd\'hui à {1,time,short}</string>
<string name="log_yesterday">{0} hier à {1,time,short}</string>
<string name="log_date">{0} {1,date,medium}</string>

<!-- These strings represent the "verbs" that will be used in the log strings
     above. -->
<string name="log_created">Créé</string>
<string name="log_changed">Modifié</string>
<string name="log_viewed">Consulté</string>
<string name="log_exported">Exporté</string>

