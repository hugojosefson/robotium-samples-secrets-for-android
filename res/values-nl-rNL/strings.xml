<string name="log_name">Toegangslog</string>
<string name="log_name_format">Toegangslog voor {0}</string>

<!-- the following string are used to format a string that indicate when a
     given secret was created or last viewed, modified, or exported.  The
     {0} will be replaced with a "verb" which describes the action, and {1}
     will be replaced with a time value.  Note that for log_min, the time
     value is simply an integer, whereas for the rest its a Date object. -->
<string name="log_sec">pas geleden {0}</string>
<string name="log_min">{1} minuten geleden {0}</string>
<string name="log_today">vandaag om {1,time,short} {0}</string>
<string name="log_yesterday">gisteren om {1,time,short} {0}</string>
<string name="log_date">{1,date,medium} {0}</string>

<!-- These strings represent the "verbs" that will be used in the log strings
     above. -->
<string name="log_created">gemaakt</string>
<string name="log_changed">aangepast</string>
<string name="log_viewed">bekeken</string>
<string name="log_exported">geëxporteerd</string>

